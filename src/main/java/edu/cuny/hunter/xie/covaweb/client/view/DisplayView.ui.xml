<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:upload='urn:import:gwtupload.client'
	xmlns:c='urn:import:com.google.gwt.user.cellview.client'
	xmlns:covaView='urn:import:edu.cuny.hunter.xie.covaweb.client.view'>

	<ui:style>
		.horizontalScroll {
			overflow: auto;
			overflow-y: hidden;
		}
	</ui:style>



	<g:HTMLPanel>
		<g:Label text="This is the Display View - All Results Go Here"
			ui:field="mainTextLabel" />
		<g:ScrollPanel styleName="{style.horizontalScroll}">
			<g:HTML text="This is the Display HTML" ui:field="mainTextHTML" />
		</g:ScrollPanel>
	</g:HTMLPanel>

</ui:UiBinder>
